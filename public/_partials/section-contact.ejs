<section id="contact" class="row contact">
	<div class="col-sm-12 row__header clearfix">
		<img src="<%- imagesUri %>icon-rock-wall.svg" class="icon" alt="Contact Brandy Stone &amp; Landscaping"/>
		<h3>Contact</h3>
	</div>
	<div class="col-sm-6 contact__info">
		<div class="contact__info-inner">
			<h2>Get In Touch</h2>
			<span class="divider"></span>
			<p>P | (570) 729-8395</p>
		</div>
	</div>
	<form action="https://formspree.io/kelscahill@gmail.com" method="POST" class="col-sm-6 contact__form">
		<input name="Name" type="text" placeholder="Name" />
		<input name="Email" type="email" placeholder="Email" />
		<textarea name="Message" placeholder="Message"></textarea>
		<button class="btn btn--green submit">Submit</button>
	</form>
</section>
